{
  "name": "react-webpack-rails-tutorial",
  "version": "2.0.0",
  "description": "GigSpot - a musician networking app",
  "main": "server-express.js",
  "engines": {
    "node": "7.8.0",
    "npm": "4.4.4",
    "yarn": "0.22.0"
  },

  "scripts": {
   "heroku-postbuild": "NODE_ENV=production npm run webpack -- -p",
   "start": "cd client && npm run webpack -- --watch",
   "test": "node_modules/.bin/karma start react/karma.conf.js",
   "webpack": "node_modules/.bin/webpack --config ./react/webpack.config.js"
 },

  "repository": {
    "url": "https://github.com/donnyray27/gig_spot",
    "type": "git"
  },

  "dependencies": {},
  "devDependencies": {},
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ]
}
